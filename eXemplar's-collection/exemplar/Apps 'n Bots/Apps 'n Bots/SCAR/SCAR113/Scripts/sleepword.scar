program SleepWord;

var
   sleepfatigue,sleep0percent,sleeptopleft,sleepbottomright:integer;
   x,y:integer;

function isSleeping: boolean;
begin
     if(FindBitmap(sleepfatigue,x,y)) then result:=true;
end;

function isSleepFatigue0: Boolean;
begin
     if(FindBitmap(sleep0percent,x,y)) then result:=true;
end;

function saveHC: Boolean;
var
  ClientCanvas, DebugCanvas, BMapCanvas: TCanvas;
  bmap: Integer;
  xstart, ystart, xend, yend: Integer;
  f: Text;
begin

     xstart:=0;
     ystart:=0;
     xend:=0;
     yend:=0;
     DebugCanvas:= GetDebugCanvas;
     ClientCanvas:= GetClientCanvas;
     bmap:= BitmapFromString(256, 40, '');

     if(FindBitmap(sleeptopleft,x,y))then
     begin
          xstart:=x;
          ystart:=y;
     end;
     
     if(FindBitmap(sleepbottomright,x,y))then
     begin
          xend:=x + 11;
          yend:=y + 9;
     end;

     if((xstart > 0) and (ystart > 0) and (xend > 0) and (yend > 0))then
     begin
          Writeln('Saving hc.bmp');
          Assign(f,'slword.txt');
          Rewrite(f);
          Writeln(f,'scar-word');
          Close(f);
          BMapCanvas:= GetBitmapCanvas(bmap);
          CopyCanvas(ClientCanvas, BMapCanvas, xstart, ystart, xend, yend, 0, 0, 256, 40);
          SaveBitmap(bmap, AppPath + 'hc.bmp');
          FreeBitmap(bmap);
          result:=true;
     end else
     begin
          Writeln('Error finding sleepword bounds');
          result:=false;
     end;
end;

function readSLWord: String;
var f: Text;
var s: String;
begin
     Assign(f,'slword.txt');
     Readln(f,s);
     Close(f);
     if(s = 'scar-word')then
         result:='';
     else
         result:=s;
end;

