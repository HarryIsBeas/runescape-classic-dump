package com.mark.rsc.tickable;

/**
 *
 * @author nathan
 */
public enum TickablePriority {

    LOW(1), NORMAL(2), HIGH(3);

    private final int priority;

    TickablePriority(int priority) {
        this.priority = priority;
    }

    public int getPriority() {
        return priority;
    }
}
